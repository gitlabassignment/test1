<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>MyTest</servlet-name>
  	<display-name>MyTest</display-name>
  	<description></description>
  	<servlet-class>com.test.MyTest</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>TeamController</servlet-name>
  	<display-name>TeamController</display-name>
  	<description></description>
  	<servlet-class>com.org.controller.TeamController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>MyTest</servlet-name>
  	<url-pattern>/MyTest</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>TeamController</servlet-name>
  	<url-pattern>/TeamController</url-pattern>
  </servlet-mapping>
</web-app>
